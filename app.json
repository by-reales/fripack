{
  "expo": {
    "name": "ShadeMap",
    "slug": "ShadeNavApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBfXiE6MbfNMqsP9wd8aZAj1Jm13ElPgPw"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Permite a esta app usar tu ubicación para mostrarte tu posición en el mapa.",
        "NSLocationAlwaysUsageDescription": "Permite a esta app usar tu ubicación en segundo plano para mostrarte tu posición en el mapa."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ecomarinos.ShadeNavApp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBfXiE6MbfNMqsP9wd8aZAj1Jm13ElPgPw"
        }
      }
    },
    "extra": {
      "API_KEY": "process.env.API_KEY",  
      "WEATHER_API_KEY": "process.env.WEATHER_API_KEY",  
      "eas": {
        "projectId": "da9ebeea-b20e-4e52-8281-6db6efe7c893"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite a esta app usar tu ubicación para mostrarte tu posición en el mapa."
        }
      ],
      "expo-router"
    ],
    "owner": "by2"
  }
}